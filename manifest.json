{
    "manifest_version": 3,
    "name": "OutlookSidebarRemover",
    "version": "1.0.0",
    "description": "Removes the sidebar from MS Outlook's site",
    "content_scripts": [
        {
            "matches": [
                "https://outlook.live.com/*"
            ],
            "js": [
                "index.js"
            ]
        }
    ],
    "action": {
        "default_icon": {
            "16": "icon.png",
            "48": "icon.png",
            "128": "icon.png"
        }
    }
}